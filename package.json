    "cross-env": "^7.0.2",
    "cuid": "^2.1.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-cache-controller": "^1.1.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.2",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.11",
    "pg": "^7.18.2",
    "stripe": "^8.89.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
